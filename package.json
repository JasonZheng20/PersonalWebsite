    "Development",
    "Virtual",
    "Reality",
    "Education"
  ],
  "author": "Jason Zheng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JasonZheng20/PersonalWebsite/issues"
  },
  "homepage": "https://github.com/JasonZheng20/PersonalWebsite#readme",
  "dependencies": {
    "express": "^4.15.2",
    "jade": "^1.11.0",
    "morgan": "^1.8.1",
    "nodemon": "^1.9.2",
    "stylus": "^0.54.5"
  }
}
